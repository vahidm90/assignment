{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "types": [
      "node",
      "vite/client"
    ]
  },
  "exclude": [
    "**/__tests__/*.ts",
    "**/__tests__/*.tsx",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.test.tsx",
    "**/*.spec.tsx"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
